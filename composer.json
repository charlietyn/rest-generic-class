{
    "name": "ronu/rest-generic-class",
    "description": "Base Class for generic restfull api in laravel",
    "version": "1.7.9",
    "type": "vcs",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ronu\\RestGenericClass\\": "src/"
        }
    },
    "authors": [
        {
            "name": "charlietyn",
            "email": "charlietyn@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/pagination": "^12.0",
        "illuminate/mail": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/validation": "^12.0"
    },
    "suggest": {
        "maatwebsite/excel": "Required for exportExcel()",
        "barryvdh/laravel-dompdf": "Required for exportPdf()",
        "nwidart/laravel-modules": "Required for module-aware permissions",
        "spatie/laravel-permission": "Required for permission models and traits"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ronu\\RestGenericClass\\Core\\Providers\\RestGenericClassServiceProvider"
            ]
        }
    }
}
